from multiprocessing.synchronize import Lock
from typing import Any, Optional

logger: Any

class PrometheusClient:
    multiproc: Any
    namespace: Any
    def __init__(self, *, namespace: str = ..., multiproc: bool = ..., multiproc_lock: Optional[Lock] = ..., multiproc_dir: Optional[str] = ...) -> None: ...
    @property
    def registry(self): ...
    @staticmethod
    def mark_process_dead(pid: int) -> None: ...
    def start_http_server(self, port: int, addr: str = ...) -> None: ...
    def generate_latest(self): ...
    @property
    def CONTENT_TYPE_LATEST(self) -> str: ...
    @property
    def Histogram(self): ...
    @property
    def Counter(self): ...
    @property
    def Summary(self): ...
    @property
    def Gauge(self): ...
